@model  List<AbidzarFrm.Rukuntangga.Entities.TbKtpRow>

<ul class="users-list clearfix" id="divWarga">
    @if (Model.Count > 0)
    {
        foreach (var item in Model)
        {
            var userImage = (item.PhotoKtp == null || string.IsNullOrEmpty(item.PhotoKtp)) ? "../../Content/adminlte/img/avatar0.png" : VirtualPathUtility.ToAbsolute("~/upload/") + item.PhotoKtp;

            <li>
                <img src="@userImage" height="150" width="150" alt="User Image" />
                <a class="users-list-name" data-toggle="modal" data-target="#AbidzarFrameModalFormEdit" href="@Url.Action("Details", "Warga", new { id = item.Id })">@item.Nama</a>
            </li>
        }
    }
</ul>

